            android:label="@string/title_activity_defend_the_eggs" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".BgMusicSer"></service>
    </application>
</manifest>